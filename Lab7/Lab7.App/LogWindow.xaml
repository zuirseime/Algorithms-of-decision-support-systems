<Window x:Class="Lab7.App.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab7.App"
        mc:Ignorable="d"
        Title="LogWindow" Height="500" Width="400">
    <Grid>
        <TextBox x:Name="output" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="0" TextWrapping="Wrap" IsReadOnly="True" AcceptsReturn="True" Padding="5"/>
    </Grid>
</Window>
